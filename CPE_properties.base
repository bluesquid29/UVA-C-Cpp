filters:
  and:
    - file.fullname.endsWith("md")
    - file.folder.containsAll("Problems")
views:
  - type: table
    name: Table
    order:
      - file.name
      - title
      - cpe_rank
      - exam_dates
      - exam_numbers
      - solved
      - topics
    sort:
      - property: exam_dates
        direction: DESC
      - property: exam_numbers
        direction: ASC
      - property: status
        direction: ASC
    columnSize:
      note.title: 144
      note.status: 77
