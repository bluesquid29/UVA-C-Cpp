#include <cstdio>

int f[5001][5001] = {0}; 

int main()
{
    f[1][0] = 1;
    for (int i = 2; i <= 5000; i++)
    {
        for (int j = 0; j < 5000; j++)  
        {
            f[i][j] += f[i-1][j] + f[i-2][j];
            f[i][j+1] += f[i][j] / 10;
            f[i][j] %= 10;
        }
    }
    
    int n;
    while (scanf("%d", &n) == 1)
    {
        printf("The Fibonacci number for %d is ", n);
        int j;
        for (j = 5000; j >= 0; j--)
        {
            if (f[n][j])
            {
                break;
            }
        }
        
        if (j == -1)
        {
            printf("0");
        }
        else
        {
            for (; j >= 0; j--)
            {
                printf("%d", f[n][j]);
            }
        }
        printf("\n");
    }
    
    return 0;
}